<p>This is the homepage</p>
<h1> Taco Flow </h1>


  <form id='questionForm' action= '/questions' method='POST'>
    <input type='text' name='title' placeholder='Enter your question here'><br>
    <input type='text' name='body' placeholder='Want to expant? Add more detail here'><br>
    <input type='submit' value='ASK'>
  </form>


<div id='all_questions'>
  <% @questions.each do |question| %>
      <p><a class='expandQuestion' href='/questions/<%= question.id %>'><%= question.title %></a><%= question.user_id %> | <%= question.created_at %>
      <div id='question_<%= question.id %>'></div>
      </p>
 <% end %>
</div>
